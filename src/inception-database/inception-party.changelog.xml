  <changeSet id="inception-party-1.0.0" author="Marcus Portmann">
    <comment>Inception - Party - 1.0.0</comment>
    <sql dbms="postgresql" endDelimiter=";">
      create schema party
    </sql>
    <sql dbms="mssql" endDelimiter=";">
      create schema party
    </sql>

    <createTable schemaName="party" tableName="contact_mechanism_types" remarks="Contact Mechanism Types">
      <column name="code" type="nvarchar(30)" remarks="The code for the contact mechanism type"/>
      <column name="locale_id" type="nvarchar(10)" remarks="The Unicode locale identifier for the contact mechanism type"/>
      <column name="sort_index" type="int" remarks="The sort index for the contact mechanism type"/>
      <column name="name" type="nvarchar(50)" remarks="The name of the contact mechanism type"/>
      <column name="plural" type="nvarchar(50)" remarks="The plural name for the contact mechanism type"/>
      <column name="description" type="nvarchar(200)" remarks="The description for the contact mechanism type"/>
    </createTable>
    <addPrimaryKey schemaName="party" tableName="contact_mechanism_types" columnNames="code,locale_id"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_types" columnName="code"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_types" columnName="locale_id"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_types" columnName="sort_index"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_types" columnName="name"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_types" columnName="plural"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_types" columnName="description"/>
    <addDefaultValue schemaName="party" tableName="contact_mechanism_types" columnName="description" defaultValue=""/>
    <createIndex indexName="contact_mechanism_types_locale_id_ix" schemaName="party" tableName="contact_mechanism_types">
      <column name="locale_id"/>
    </createIndex>

    <createTable schemaName="party" tableName="contact_mechanism_purposes" remarks="Contact Mechanism Purposes">
      <column name="type" type="nvarchar(30)" remarks="The code for the contact mechanism type the contact mechanism purpose is associated with"/>
      <column name="code" type="nvarchar(30)" remarks="The code for the contact mechanism purpose"/>
      <column name="locale_id" type="nvarchar(10)" remarks="The Unicode locale identifier for the contact mechanism purpose"/>
      <column name="sort_index" type="int" remarks="The sort index for the contact mechanism purpose"/>
      <column name="name" type="nvarchar(50)" remarks="The name of the contact mechanism purpose"/>
      <column name="description" type="nvarchar(200)" remarks="The description for the contact mechanism purpose"/>
      <column name="party_types" type="nvarchar(300)" remarks="The comma-delimited list of codes for the party types the contact mechanism purpose is associated with"/>
    </createTable>
    <addPrimaryKey schemaName="party" tableName="contact_mechanism_purposes" columnNames="code,locale_id"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_purposes" columnName="type"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_purposes" columnName="code"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_purposes" columnName="locale_id"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_purposes" columnName="sort_index"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_purposes" columnName="name"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_purposes" columnName="description"/>
    <addNotNullConstraint schemaName="party" tableName="contact_mechanism_purposes" columnName="party_types"/>
