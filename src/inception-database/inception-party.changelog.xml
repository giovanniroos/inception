      baseTableName="contact_mechanism_purposes" baseColumnNames="type,locale_id" referencedTableSchemaName="reference"
      referencedTableName="contact_mechanism_types" referencedColumnNames="code,locale_id" deleteCascade="true"/>
    <addDefaultValue schemaName="party" tableName="contact_mechanism_purposes" columnName="description" defaultValue=""/>
